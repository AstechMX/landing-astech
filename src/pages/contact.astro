---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "ASTECH - Contacto: Conéctate para Soluciones Web Innovadoras",
	description:
		"Contacta a ASTECH para servicios expertos en diseño web y SEO. Conéctate con nosotros para discutir tu proyecto y explorar soluciones web de vanguardia.",
	canonical: "https://astech.dev/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
</BaseLayout>
